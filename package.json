{
  "name": "meteor-steel-site-2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@anthropic-ai/sdk": "^0.75.0",
    "@capacitor/android": "^8.1.0",
    "@capacitor/app": "^8.0.1",
    "@capacitor/cli": "^8.1.0",
    "@capacitor/core": "^8.1.0",
    "@capacitor/haptics": "^8.0.1",
    "@capacitor/ios": "^8.1.0",
    "@capacitor/keyboard": "^8.0.1",
    "@capacitor/push-notifications": "^8.0.1",
    "@capacitor/share": "^8.0.1",
    "@capacitor/splash-screen": "^8.0.1",
    "@capacitor/status-bar": "^8.0.1",
    "@react-google-maps/api": "^2.20.8",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/xr": "^6.6.29",
    "astronomy-engine": "^2.1.19",
    "cesium": "^1.138.0",
    "firebase": "^12.9.0",
    "firebase-admin": "^13.6.1",
    "jspdf": "^4.2.0",
    "openai": "^4.78.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^6.30.3",
    "react-scripts": "5.0.1",
    "resium": "^1.19.4",
    "stripe": "^20.3.1",
    "three": "^0.182.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "CI=false react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "cap:sync": "npm run build && npx cap sync",
    "cap:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "cap:android": "npm run build && npx cap sync android && npx cap open android",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "https://meteor-steel-site-2.vercel.app",
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mammoth": "^1.11.0",
    "react-app-rewired": "^2.2.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npx react-app-rewired test --watchAll=false --bail --findRelatedTests"
    ],
    "src/data/**/*.json": [
      "npx react-app-rewired test --watchAll=false --bail --testPathPattern=dataIntegrity"
    ]
  }
}
